#!/bin/bash

# First reset all settings
for schema in $(gsettings list-schemas); do
  gsettings reset-recursively "$schema"
done
gsettings reset-recursively org.gnome.Terminal.Legacy.Keybindings:/org/gnome/terminal/legacy/keybindings/

# Now set everything I like.
gsettings set org.gnome.Terminal.Legacy.Keybindings:/org/gnome/terminal/legacy/keybindings/ close-tab '<Alt>w'
gsettings set org.gnome.Terminal.Legacy.Keybindings:/org/gnome/terminal/legacy/keybindings/ close-window ''
gsettings set org.gnome.Terminal.Legacy.Keybindings:/org/gnome/terminal/legacy/keybindings/ copy ''
